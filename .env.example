# Ejemplo de archivo .env
# Copia este archivo como .env y rellena con tus valores

# URL de tu instancia n8n (sin barra final)
N8N_API_URL=https://tu-instancia.n8n.cloud

# API Key de n8n (generar desde Settings → API)
N8N_API_KEY=tu_api_key_aqui

# Evolution API (para scripts de automação WhatsApp)
EVOLUTION_API_URL=http://localhost:8080
EVOLUTION_API_KEY=SUA_API_KEY_EVOLUTION

# Automação cadastro de imóveis (Sheets/Drive/Notion)
GOOGLE_DRIVE_FOLDER_ID=SEU_FOLDER_ID_AQUI
GOOGLE_SHEETS_ID=SEU_SPREADSHEET_ID_AQUI
GOOGLE_SHEETS_NAME=Prospecção
NOTION_DATABASE_ID=SEU_DATABASE_ID_NOTION

# Automação prospecção Trello
TRELLO_BOARD_ID=SEU_BOARD_ID
TRELLO_LIST_NOVOS_ID=SEU_LIST_NOVOS_ID

# Automação simples Telegram -> Trello
# Use o chat ID do grupo para limitar o uso (opcional)
TELEGRAM_ALLOWED_CHAT_ID=
